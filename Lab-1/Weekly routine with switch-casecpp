#include <iostream>
using namespace std;
int main(){
	int a;
	cout<<"Select the day of the week(1-7): ";
	cin>>a;
	switch (a) {
	case 1:
		cout<<"Library";
		break;
	case 2:
		cout<<"Boxing";
		break;
	case 3:
		cout<<"Lesson";
		break;
	case 4:
		cout<<"Boxing";
		break;
	case 5:
		cout<<"Break/Rest";	
		break;
	case 6:
		cout<<"Boxing";
		break;
	case 7:
		cout<<"General/Review";
		break;
	default:
		cout<<""Invalid input! Please enter a number between 1 and 7.";
		break;
}			
}
